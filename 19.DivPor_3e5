#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

main (){
	int n;
	printf("\nInforme um numero: ");
	scanf("%d",&n);
	if ((n%5==0) && !(n%3==1))
		printf("\nE divisivel por 5 e nao por 3");
	else
		if ((n%3==0) && !(n%5==1))
			printf("\nE divisivel por 3 e nao por 5");
}
